<div class="row-fluid">
	
	<div class='span8 offset2'>
		<% @feed.each do |post| %>
			<div class='post'>
				<img src=<%= post['picture'] %> class="img-rounded">
				<p class='name'><%= post['name'] %>
				<p hidden class='source'><%= post['source'] %>
			</div>
			</br>
		<% end %>
	</div>

	<div class='span0'>
		<iframe id="playa" src="<%= @feed[0]['source'] %>" width="100%" height="500px" hidden> </iframe>
		</br>
	</div>

</div>