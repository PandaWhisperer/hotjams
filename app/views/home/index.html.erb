<% @feed.each do |post| %>
	<div class='post row-fluid'>
		<div class='span8 offset2'>
			<img src=<%= post['picture'] %> class="img-circle">
			<p class='name'><%= post['name'] %></p>
			<p hidden class='source'><%= post['source'] %></p>
		</div>
		</br>
	</div>
<% end %>
	

<div class='span0'>
	<iframe id="playa" src="<%= @feed[0]['source'] %>" width="100%" height="500px" hidden> </iframe>
	</br>
</div>
